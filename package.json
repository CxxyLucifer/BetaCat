{
  "name": "betaCat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "reset_cache": "npm start --reset-cache",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rm -rf ./node_modules && yarn",
    "format": "./node_modules/.bin/prettier --write './src/**/*.{ts,tsx}'",
    "build_apk": "cd android && ./gradlew assembleRelease",
    "android": "node node_modules/react-native/local-cli/cli.js run-android && ./node_modules/.bin/tsc -w",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator 'iPhone X' && ./node_modules/.bin/tsc -w"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "debounce": "^1.2.0",
    "deepmerge": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "native-base": "^2.12.1",
    "object-assign": "^4.1.1",
    "plume2": "^1.2.7",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-mixin": "^5.0.0",
    "react-native": "0.59.9",
    "react-native-drawer-layout": "^2.0.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-safe-area-view": "^0.14.5",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0",
    "rn-topview": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.62",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.0",
    "react-test-renderer": "16.8.3",
    "tslib": "^1.10.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
